{
  "name": "airbnb-clone",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn api:generate-client",
    "prepare": "husky install",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "run:api": "yarn api:gen-graphql-typings && nx serve airbnb-clone-api",
    "lint:api": "nx lint airbnb-clone-api",
    "build:api": "rm -rf dist/apps/airbnb-clone-api && yarn api:gen-graphql-typings && nx build airbnb-clone-api --prod",
    "run:api:ci": "pm2-runtime pm2.config.js",
    "lint:fix:api": "nx lint airbnb-clone-api",
    "test:api": "nx test airbnb-clone-api",
    "api:generate-client": "npx prisma generate --schema ./apps/airbnb-clone-api/prisma/schema.prisma",
    "api:gen-graphql-typings": "ts-node ./apps/airbnb-clone-api/src/graphql/generateTypings.ts"
  },
  "private": true,
  "dependencies": {
    "@apollo/gateway": "^0.44.1",
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/terminus": "^8.0.3",
    "@nestjs/throttler": "^2.0.0",
    "@prisma/client": "^3.6.0",
    "apollo-server-express": "^3.5.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "graphql": "^15",
    "graphql-tools": "^8.2.0",
    "morgan": "^1.10.0",
    "pm2": "^5.1.2",
    "prisma": "^3.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "swagger-ui-express": "^4.2.0",
    "ts-morph": "^13.0.2",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.2.3",
    "@nrwl/eslint-plugin-nx": "13.2.3",
    "@nrwl/jest": "13.2.3",
    "@nrwl/linter": "13.2.3",
    "@nrwl/nest": "13.2.3",
    "@nrwl/node": "13.2.3",
    "@nrwl/tao": "13.2.3",
    "@nrwl/workspace": "13.2.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "@typescript-eslint/parser": "~4.33.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "husky": "^7.0.4",
    "jest": "27.2.3",
    "lint-staged": ">=10",
    "prettier": "^2.5.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.4.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
